{
  "name": "@gerritkit/native",
  "version": "1.2.3",
  "publishConfig": {
    "access": "public"
  },
  "description": "Native client for gerrit",
  "keywords": [],
  "main": "target/es5/index.js",
  "source": "target/ts/index.ts",
  "types": "target/es5/index.d.ts",
  "typescript": {
    "definition": "target/es5/index.d.ts"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "target",
    "typings",
    "flow-typed"
  ],
  "scripts": {
    "test": "yarn infra:npm-run-all -p -l  lint test:spec",
    "test:spec": "yarn jest",
    "jest": "jest -w=1 --config=jest.config.json",
    "format": "yarn infra:prettier --write 'src/**/*.ts'",
    "lint": "yarn infra:eslint src/main/ts/**/*.ts",
    "lint:fix": "yarn lint --fix",
    "clean": "yarn infra:rimraf target typings flow-typed buildcache coverage docs buildstamp.json",
    "test:depcheck": "yarn infra:depcheck --ignores='typedoc,tslib,earljs,eslint-*,prettier-*,**/*-infra' --ignore-patterns='typings'",
    "prebuild": "yarn infra:buildstamp --git --out.path buildstamp.json",
    "build": "yarn infra:npm-run-all -p -l build:es5 build:es6 build:ts build:libdef docs uglify",
    "build:es5": "yarn infra:mkdirp target/es5 && yarn infra:tsc -p tsconfig.es5.json",
    "build:es6": "yarn infra:mkdirp target/es6 && yarn infra:tsc -p tsconfig.es6.json",
    "build:ts": "cpy src/main/ts/ target/ts/",
    "build:libdef": "yarn infra:libdefkit --tsconfig=tsconfig.es5.json --tsconfig=tsconfig.es6.json",
    "docs": "typedoc --out docs/typedoc ./ --readme README.md --tsconfig tsconfig.es5.json src/main",
    "uglify": "yarn infra:uglify",
    "postupdate": "yarn && yarn build && yarn test"
  },
  "dependencies": {
    "@gerritkit/core": "workspace:*",
    "@qiwi/substrate": "^1.20.6",
    "@types/cheerio": "0.22.22",
    "axios": "0.21.1",
    "cheerio": "1.0.0-rc.3",
    "lodash.omit": "^4.5.0",
    "tslib": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gerritkit/client.git"
  },
  "author": "Anton Golub <mailbox@antongolub.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gerritkit/client/issues"
  },
  "homepage": "https://github.com/gerritkit/client/#readme",
  "devDependencies": {
    "@types/lodash.omit": "^4.5.6",
    "axios-mock-adapter": "^1.19.0"
  }
}
